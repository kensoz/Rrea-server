    "koa-body": "^4.2.0",
    "koa-router": "^10.1.1"
  },
  "devDependencies": {
    "@types/koa": "^2.13.4",
    "@types/koa-logger": "^3.1.2",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^17.0.16",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "dayjs": "^1.10.7",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "koa-logger": "^3.2.1",
    "log4js": "^6.4.1",
    "mongoose": "^6.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.5"
  }
